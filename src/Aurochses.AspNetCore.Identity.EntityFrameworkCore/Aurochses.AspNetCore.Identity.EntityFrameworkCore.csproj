<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Aurochses.AspNetCore.Identity.EntityFrameworkCore</PackageId>
    <Version>2.2.0</Version>
    <Authors>Aurochses</Authors>
    <Company>Aurochses</Company>
    <Description>Aurochses.AspNetCore.Identity.EntityFrameworkCore is a library for ASP.NET Core Identity using Entity Framework Core.</Description>
    <Copyright>Aurochses 2017</Copyright>
    <PackageLicenseUrl>https://github.com/Aurochses/Aurochses.AspNetCore.Identity.EntityFrameworkCore/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Aurochses/Aurochses.AspNetCore.Identity.EntityFrameworkCore/wiki</PackageProjectUrl>
    <PackageIconUrl>https://www.gravatar.com/avatar/cb5925a77e502401f95bf2738b123994?s=64</PackageIconUrl>
    <RepositoryUrl>https://github.com/Aurochses/Aurochses.AspNetCore.Identity.EntityFrameworkCore</RepositoryUrl>
    <PackageTags>Entity Framework Core Identity ASP.NET Core</PackageTags>

    <!-- SonarQube needs this -->
    <ProjectGuid>{00000000-0000-0000-0000-000000000001}</ProjectGuid>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Aurochses.AspNetCore.Identity" Version="2.2.0" />
    <PackageReference Include="Aurochses.Data.EntityFrameworkCore" Version="2.2.0" />
    <PackageReference Include="Aurochses.Data.Extensions.MsSql" Version="2.2.0" />
    <PackageReference Include="IdentityModel" Version="3.10.5" />
    <PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="2.2.0" />
  </ItemGroup>

</Project>
